{
  "version": "0.0.2",
  "name": "@typescript-templates/node-express-rest-api",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postpublish": "git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "commit-release": "git commit -a -m \"Release v%npm_package_version%\" && git push && git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "build-tsc": "tsc",
    "build": "gulp build",
    "test-mocha": "node node_modules/mocha/bin/mocha --require ts-node/register test/*.test.ts --reporter spec",
    "test": "gulp test-mocha",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.test|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "npm-publish": "npm publish",
    "test-jest": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
    "watch-debug-serve": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "description": "Starter for TypeScript with eslint and gulp",
  "keywords": [
    "typescript",
    "starter",
    "gulp",
    "eslint",
    "ghpages",
    "travis",
    "git auto tags",
    "git auto commit"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@dotup/dotup-ts-mongoose": "1.1.2",
    "@dotup/dotup-ts-types": "1.1.1",
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.19",
    "lusca": "^1.6.1",
    "mongodb": "3.5.5",
    "mongoose": "5.9.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "recursive": "^0.0.2",
    "validator": "^13.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^5.1.0",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^6.1.8",
    "@types/express": "4.17.1",
    "@types/jsonwebtoken": "8.3.8",
    "@types/lusca": "1.6.1",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "3.5.3",
    "@types/mongoose": "5.7.7",
    "@types/node": "^12.12.31",
    "@types/passport": "^1.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/shelljs": "^0.8.7",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "concurrently": "5.1.0",
    "cross-spawn": "^7.0.1",
    "del": "^5.1.0",
    "dotup-gulp-gh-pages": "0.6.0-6",
    "dotup-ts-git-wrapper": "^1.0.0",
    "eslint": "6.8.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "6.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-mocha": "^7.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typedoc": "^2.2.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^7.1.1",
    "nodemon": "2.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "8.8.1",
    "typedoc": "^0.17.3",
    "typescript": "3.8.3"
  },
  "author": {
    "name": "Peter Ullrich",
    "email": "dotup.software@gmail.com"
  },
  "license": "GNU General Public License v3.0",
  "repository": {
    "url": "https://github.com/typescript-templates/node-express-rest-api"
  }
}
