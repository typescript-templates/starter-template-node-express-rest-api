{
  "version": "0.0.2",
  "name": "@typescript-templates/node-express-rest-api",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postpublish": "git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "commit-release": "git commit -a -m \"Release v%npm_package_version%\" && git push && git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "build-tsc": "tsc",
    "build": "gulp build",
    "test-mocha": "node node_modules/mocha/bin/mocha --require ts-node/register test/*.test.ts --reporter spec",
    "test": "gulp test-mocha",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.test|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "npm-publish": "npm publish",
    "test-jest": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\"",
    "watch-debug-serve": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "description": "Starter for TypeScript with eslint and gulp",
  "keywords": [
    "typescript",
    "starter",
    "gulp",
    "eslint",
    "ghpages",
    "travis",
    "git auto tags",
    "git auto commit"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@dotup/dotup-ts-types": "0.4.1",
    "@dotup/dotup-ts-mongoose": "1.1.1",
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "compression": "1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "mongodb": "3.5.3",
    "mongoose": "^5.9.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "recursive": "^0.0.2",
    "validator": "^12.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.9",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^5.1.0",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^6.1.8",
    "@types/express": "^4.17.2",
    "@types/express-serve-static-core": "4.17.2",
    "@types/jsonwebtoken": "7.2.7",
    "@types/lusca": "1.6.1",
    "@types/mocha": "^7.0.1",
    "@types/mongodb": "3.1.31",
    "@types/mongoose": "5.5.43",
    "@types/node": "^13.7.6",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.45",
    "@types/shelljs": "^0.8.6",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "concurrently": "5.1.0",
    "cross-spawn": "^7.0.1",
    "del": "^5.1.0",
    "dotup-gulp-gh-pages": "0.6.0-6",
    "dotup-ts-git-wrapper": "^1.0.0",
    "eslint": "6.8.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "6.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-mocha": "^7.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typedoc": "^2.2.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^7.1.0",
    "nodemon": "2.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.2"
  },
  "author": {
    "name": "Peter Ullrich",
    "email": "dotup.software@gmail.com"
  },
  "license": "GNU General Public License v3.0",
  "repository": {
    "url": "https://github.com/typescript-templates/node-express-rest-api"
  }
}